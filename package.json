{
  "name": "terraform-version-x-ray",
  "displayName": "Terraform Version X-Ray",
  "description": " View current version information for terraform providers and modules inline with your code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "languages": [
    {
      "id": "hcl",
      "extensions": [
        ".hcl",
        ".tf"
      ],
      "aliases": [
        "HCL",
        "hcl",
        "tf",
        "Terraform",
        "terraform"
      ]
    }
  ],
  "contributes": {
    "commands": [
      {
        "command": "terraform-version-x-ray.enableCodeLens",
        "title": "Enable Terraform Version X-Ray",
        "category": "Terraform"
      },
      {
        "command": "terraform-version-x-ray.disableCodeLens",
        "title": "Disable Terraform Version X-Ray",
        "category": "Terraform"
      }
    ],
    "configuration": {
      "properties": {
        "terraform-version-x-ray.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/semver": "^7.3.9",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.12.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "got": "^12.0.3",
    "semver": "^7.3.5"
  }
}
